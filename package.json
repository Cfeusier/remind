{
  "name": "remind-cli",
  "version": "0.0.3",
  "preferGlobal": "true",
  "description": "Remind-CLI will send text messages of your choosing at given intervals",
  "author": "Clark Feusier <cfeusier@gmail.com>",
  "license": "MIT",
  "bin": {
    "remind-cli": "./bin/remind"
  },
  "scripts": {
    "build": "WEBPACK_ENV=build webpack",
    "dev": "WEBPACK_ENV=dev webpack --progress --colors --watch",
